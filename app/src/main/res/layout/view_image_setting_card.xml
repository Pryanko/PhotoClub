<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:soundEffectsEnabled="false"
    tools:ignore="KeyboardInaccessibleWidget,SpUsage">

    <com.photoprint.photoclub.ui.widget.CustomButton
        android:id="@+id/nextButton"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/bg_yellow_btn"
        android:elevation="4dp"
        android:text="@string/setting_card_btn_next"
        android:textAllCaps="true"
        android:textSize="14dp"
        android:theme="@style/Widget.App.Button.Yellow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpUsage" />

</android.support.constraint.ConstraintLayout>